    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug gst-launch with 'gstwaylanddisplay'",
            "env": {
                "RUST_BACKTRACE": "1",
                "GST_PLUGIN_PATH": "target/x86_64-unknown-linux-gnu/debug:${env:GST_PLUGIN_PATH}",
                "GST_DEBUG": "2,waylanddisplaysrc=7"
            },
            "program": "/usr/bin/gst-launch-1.0",
            "args": ["-v", "waylanddisplaysrc", "!", "videoconvert", "!", "x264enc", "tune=zerolatency", "speed-preset=superfast", "!", "autovideosink"],
            "cwd": "${workspaceFolder}"
        }
    ]
}